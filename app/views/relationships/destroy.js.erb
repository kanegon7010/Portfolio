document.getElementById("follow_form_<%= @user.id %>").innerHTML = "<%= escape_javascript( render partial: "users/follow", locals: { user: @user }) %>";
if (document.getElementById("rscounter_<%= @user.id %>") != null)  {
  document.getElementById("rscounter_<%= @user.id %>").innerHTML = "<%= j(render( partial: "users/relationshipcounter", locals: { user: @user })) %>";
}
